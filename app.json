{
  "expo": {
    "name": "OngOr",
    "slug": "OngOr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo/LogoRound.png",
    "owner": "nice2250",
    "userInterfaceStyle": "light",
    "scheme": "ongor",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ongor.fun",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Logo/LogoRound.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ongor.fun",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/Logo/LogoRound.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/Logo/LogoRound.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5638b405-59a2-48ed-aa55-14aec6da9dde"
      },
      "google": {
        "webClientId": "800080342020-iq03he55phpkk5rs5bmsvk2pc0s8huni.apps.googleusercontent.com"
      }
    }
  }
}
